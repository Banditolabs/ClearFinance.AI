  "description": "A financial dashboard with locally run ai",
  "license": "ISC",
  "author": "Joshuagosst@gmail.com",
  "type": "commonjs",
  "main": "electron/main.ts",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:vite\" \"npm:dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "electronmon .",
    "type-check": "tsc --noEmit",
